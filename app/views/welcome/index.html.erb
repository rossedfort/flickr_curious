<div class="container center top-margin">
  <h1>Welcome to flickr curious :D</h1>
  <% if current_user %>
    <p>hello, <%= current_user.name %></p>
    <%= link_to "logout", logout_path %>
  <% else %>
    <%= link_to "login", login_path %>
  <% end %>
</div>
<div class="container">
  <% @photos.each do |photo| %>
    <%= image_tag FlickRaw.url_m(photo) %>
  <% end %>
</div>
